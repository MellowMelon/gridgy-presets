{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {"jsx": true},
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "block-scoped-var": 2,
    "dot-notation": 0,
    "eqeqeq": [2, "smart"],
    "no-alert": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-const-assign": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": [2, {"commentPattern": ".*"}],
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-lone-blocks": 2,
    "no-nested-ternary": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-restricted-syntax": [2, "WithStatement"],
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-template-curly-in-string": 2,
    "no-throw-literal": 2,
    "no-underscore-dangle": 0,
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-vars": [2, {"args": "none"}],
    "no-var": 2,
    "no-with": 2,
    "prefer-const": [2, {"destructuring": "all"}],
    "prefer-rest-params": 2,
    "prefer-spread": 2,
    "require-await": 2,
    "strict": 0,
    "use-isnan": 2,
    "valid-typeof": [2, {"requireStringLiterals": true}]
  }
}
